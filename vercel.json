{
  "version": 2,
  "buildCommand": "node scripts/build-client-only.js",
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.ts"
    }
  ]
}
